%body{:id=>'checkout',:class=>'checkout'}
   #screen.wrapper
      #display
         #frame1.deep1.active
            .deep2
               .content
                  .top.title
                     %h1 Let’s get Start
                  .detail
                     .col-xs-6
                        %h2 How many snacks per box?
                        %ul.labels.list
                           %li.label.l1
                              %a{:href=>'#'}
                                 +5
                                 %span snacks
                           %li.label.l1.active
                              %a{:href=>'#'}
                                 +10
                                 %span snacks
                           %li.label.l1
                              %a{:href=>'#'}
                                 +20
                                 %span snacks
                     .col-xs-6
                        %h2 Choose your delivery frequency
                        %ul.times.list
                           %li.time.t1
                              %a{:href=>'#'}
                                 weekly
                           %li.time.t1.active
                              %a{:href=>'#'}
                                 every 2 weeks
                           %li.time.t1
                              %a{:href=>'#'}
                                 monthly
                  .control
                     %a.border.btn.next{:href=>'#'} Next
         #frame2.deep1
            .deep2
               .content
                  .top.title
                     %h1 Customize this package
                     %p.min.text (optional for picky eaters or people with allergies)
                  .detail
                     %ul.labels.list
                        %li.label.item
                           .cycle Spicy
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Salty
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Sweet
                           %a.minu -
