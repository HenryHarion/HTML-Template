                           %a.plus +
                        %li.label.item
                           .cycle Chewy
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Crunchy
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Gummy
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Seafood
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Nuts
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Meat
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Candy
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Chips
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Cookies
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Foreign Snacks
                           %a.minu -
                           %a.plus +
                        %li.label.item
                           .cycle Domestic Snacks
                           %a.minu -
                           %a.plus +
                  .control
                     %p Above Variables ($1 Each)
                     %a.border.btn.down{:href=>'#'} v
         #frame3.deep1
            .deep2
               .content
                  .top.title
                     %h1 Billing Address
                     %p.min.text
                        Already have an account?
                        %a#quicklogin.min.red.btn{:href=>'#'}
                           Sign in!
                  .detail
                     %form#billing_address
                        .col-xs-6
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'firstname',:value=>' '}
                                 .label
                                    First name
                                    %p.red.req *
                              .input.object.right.text
                                 %input{:type=>'textbox',:name=>'lastname',:value=>' '}
                                 .label
                                    Last name
                                    %p.red.req *
                           .inputs.line
                              .input.object.fit.text
                                 %input{:type=>'textbox',:name=>'address',:value=>' '}
                                 .label
                                    Address
                                    %p.red.req *
                           .inputs.line
                              .input.object.fit.text
                                 %input{:type=>'textbox',:name=>'address2',:value=>' '}
                                 .label
                                    Address 2
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'tel',:value=>' '}
                                 .label
                                    Tel
                                    %p.red.req *
                              .input.object.right.text
                                 %input{:type=>'textbox',:name=>'email',:value=>' '}
                                 .label
                                    Email
                                    %p.red.req *
                        .col-xs-6
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'city',:value=>' '}
                                 .label
                                    City
                                    %p.red.req *
                              .input.object.right.text
                                 %select{:name=>'region',:value=>' '}
                                    %option{:value=>'0'}
                                    %option{:value=>'1'} Region
                                    %option{:value=>'2'} State
                                    %option{:value=>'3'} Province
                                 .label
                                    Region, state or province
                                    %p.red.req *
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'zip',:value=>' '}
                                 .label
                                    Zip
                                    %p.red.req *
                              .input.object.right.text
                                 %select{:name=>'state',:value=>' '}
                                    %option{:value=>'0'}
                                    %option{:value=>'1'} United State
                                    %option{:value=>'2'} Japanese
                                    %option{:value=>'3'} Vietnamese
                                 .label
                                    Country
                                    %p.red.req *
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'company',:value=>' '}
                                 .label
                                    Company
                              .input.object.right.text
                                 %input{:type=>'textbox',:name=>'fax',:value=>' '}
                                 .label
                                    Fax
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'password',:value=>' '}
                                 .label
                                    Password
                                    %p.red.req *
                              .input.object.right.text
                                 %input{:type=>'textbox',:name=>'confirm',:value=>' '}
                                 .label
                                    Confirm Password
                                    %p.red.req *
                           .inputs.line.checking
                              %span
                                 %input.checkbox{:type=>'checkbox',:checked=>'checked',:name=>'same'}
                                 CHECK IF BILLING AND SHIPPING ADDRESS ARE THE SAME
                  .control
                     %a.border.btn.down{:href=>'#'} v
         #frame4.deep1
            .deep2
               .content
                  %form#billing_address
                     .top.title
                        %h1 Shipping Address
                     .detail
                        .col-xs-6
                           .inputs.line
