                                 %input{:type=>'textbox',:name=>'confirm',:value=>' '}
                                 .label
                                    Confirm Password
                                    %p.red.req *
                     .control
                        %a.border.btn.down{:href=>'#'} v
         #frame4.deep1
            .deep2
               .content
                  %form#shipping_address
                     .top.title
                        %h1 Your SnackPak is ready
                     .detail
                        .col-xs-6
                           .inputs.line
                              %h2 Payment Method
                           .inputs.line
                              %ul.cards.list
                                 %li.card.item.c1
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_paypal.png'}
                                 %li.card.item.c2
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_visa.png'}
                                 %li.card.item.c3.active
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_master.png'}
                                 %li.card.item.c4
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_american.png'}
                                 %li.card.item.c5
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_discover.png'}
                                 %li.card.item.c6
                                    %a{:href=>'#'}
                                       %img{:src=>'./images/01_Checkout/card_bitcoin.png'}
                           .inputs.line
                              .input.object.fit.text
                                 %input{:type=>'textbox',:name=>'cardname',:value=>' '}
                                 .label
                                    Name on Card
                           .inputs.line
                              .input.object.fit.text
                                 %input{:type=>'textbox',:name=>'cardnumber',:value=>' '}
                                 .label
                                    Card Number
                           .inputs.line
                              .input.object.left.text
                                 %input{:type=>'textbox',:name=>'expired',:value=>' '}
                                 .label
                                    Expired date
                              .input.object.right.text
                                 %input{:type=>'textbox',:name=>'cvv',:value=>' '}
                                 .label
                                    CVV
                        .col-xs-6
                           .inputs.line
                              %h2 Shipping Method
                           .inputs.line
                              .dashed.border
                                 .subcontent
                                    .big.text Flat rate
                                    .coin.text
                                       .currency $
                                       .big.number 12
                                       .small.number 00
                                    .min.text Purchase multiple months and save on shipping!
                           .inputs.line
                              %h2 Apply Promo Code
                           .inputs.line
                              .input.object.fit.text
                                 %input{:type=>'textbox',:name=>'promocode',:value=>' '}
                                 .label
                                    Enter your Promo code
                                    %p.green.checked
                     .control
                        %a.border.btn.down{:href=>'#'} Order Now
                        %p.sub.text By clicking ORDER, you agree to the TERMS OF USE
         #frame4.deep1
            .deep2
               .content
                  .top.title
                     %h1 Check out Sucessfully
                     %h2 Your MunchPak is on the way
                  .detail
                     .special.line.l1
                        %h3.red Total amount
                     .special.line.l2
                        %h4.black Shipping & Promo Included
                     .special.line.l3
                        .amount
                           .currency $
                           .big.number 230
                           .small.number 25
                     .special.line.l4
                        %p.grey Share URL & Receive 50 MunchPak Points for every friend you refer
                     .special.line.l5
                        %a.red{:href=>'https://munchpak.com/onepage/',:target=>'_blank'} https://munchpak.com/onepage/
                  .control
                     %h3 Or share this url on
                     %ul.social.small.list
                        %li.social.ico.item
                           %a{:href=>'#',:target=>'_blank'}
                              %img{:src=>'./images/01_Checkout/mini_facebook.png'}
                        %li.social.ico.item
                           %a{:href=>'#',:target=>'_blank'}
                              %img{:src=>'./images/01_Checkout/mini_twitter.png'}
                        %li.social.ico.item
                           %a{:href=>'#',:target=>'_blank'}
                              %img{:src=>'./images/01_Checkout/mini_google.png'}
                     %a.red.big.btn{:href=>'#',:target=>'_blank'} Visit our Shop
   #login.scene.s0
      .content
         .top.title
            %h1 Login required
            %p.min.text You have to login to continue your checkout process.
         .detail
            %form#quick_login{:name=>'quicklogin',:method=>'post',:action=>'#'}
               %input#username.textbox{:type=>'text',:placeholder=>'Username',:value=>' '}
               %input#password.textbox{:type=>'text',:placeholder=>'Password',:value=>' '}
               %input#login.submit.btn{:type=>'submit',:value=>'login'}
         .control
            .note.sub.text
               Don’t have an account?
               %a{:href=>'#'} Sign up now
               or just use your available accounts:
            .social.bar
               %a.facebook.left{:href=>'#',:target=>'_blank'}
                  %img.ico{:src=>'./images/01_Checkout/ico_facebook.png',:rel=>'Login via Facebook'}
               %a.google.right{:href=>'#',:target=>'_blank'}
                  %img.ico{:src=>'./images/01_Checkout/ico_google.png',:rel=>'Login via Google'}
   #controller
      #back
         %a#go_back.square.btn.min{:href=>'#'}
            <
      #scroller
         .main.content
            .currency $
            .big.number 230
            .small.number 25
         .sub.content
            %ul.steps.check
               %li.item.step1.active
                  %a{:href=>'#1'} 1
               %li.item.step2
                  %a{:href=>'#1'} 2
               %li.item.step3
